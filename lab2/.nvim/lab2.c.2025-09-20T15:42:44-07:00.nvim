#define _POSIX_C_SOURCE 200800L
#define _GNU_SOURCE
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {

  char *line = NULL, *token = NULL, *saveptr;
  size_t len =0;
  size_t read;

  printf("Enter programs to run:");
  read = getline(&line, &len, stdin);
  if(read != -1){

    token = strtok_r(line, " ", &saveptr);
    printf("Toekns:\n");
    while(token != NULL){
      printf(" %s\n", token);
      token = strtok_r(NULL, " ", &saveptr);
    }
  }
}
