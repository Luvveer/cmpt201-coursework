#define _POSIX_C_SOURCE 200800L
#define _GNU_SOURCE
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {

  char *line = NULL, *saveptr;
  size_t len = 0;
  size_t read;

  printf("Please enter some text: ");
  read = getline(&line, &len, stdin);
  if (read != -1) {
    while (line != NULL) {
      line = strtok_r(line, " ", &saveptr);
      printf("%s\n", line);
    }
  }
  return 0;
}
